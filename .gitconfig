[init]
	defaultBranch = main
[user]
	email = a.hr.nikolov@gmail.com
	name = Atanas Nikolov
[pull]
	rebase = false
[core]
  pager = less -FXRS
	editor = code
[color]
	ui = true
[merge]
  tool = code
[rebase]
	autoStash = true
[alias]
	logline = log --oneline --abbrev-commit --all --decorate --graph
	prettylog = log --pretty='%C(red)%h%Creset%C(yellow)%d%Creset %s %C(cyan)(%ar)%Creset'
	new = prettylog main..HEAD
	missing = prettylog HEAD..main
